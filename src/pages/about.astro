---
interface IData {
  title: string;
  bodytext: string;
  image: string;
}
import DefaultLayout from "../layouts/default.astro";
import Banner from "../components/banner.astro";
import data from "../data/about.json";

const { title, bodytext, image } = data as IData;
---

<DefaultLayout title={title}>
  <Banner title={title} bodytext={bodytext} image={image} />
</DefaultLayout>
